{
    "name": "geeks-diary",
    "version": "1.0.0",
    "description": "Geek's Diary - TIL writing tool",
    "main": "dist/main.js",
    "scripts": {
        "start:main": "npx webpack --config configs/webpack.electron.js --watch",
        "start:renderer": "npx webpack --config configs/webpack.browser.dev.js --watch",
        "build:main": "RUN_TARGET=production npx webpack --config configs/webpack.electron.js",
        "build:renderer": "RUN_TARGET=production npx webpack --config configs/webpack.browser.prod.js",
        "launch": "npx electron --inspect=5858 .",
        "clean:dist": "npx rimraf dist/",
        "clean:packages": "npx rimraf packages/",
        "ci:travis": "npm run test && npm run build:main && npm run build:renderer",
        "lint": "npm run lint:renderer && npm run lint:main",
        "lint:renderer": "npx tslint --project src/tsconfig.app.json",
        "lint:main": "npx tslint --project src/tsconfig.electron.json",
        "test": "npm run lint && npx karma start karma.conf.js",
        "package": "npx electron-builder -mwl",
        "tools:copy-vendors": "tools/copy-vendors.sh",
        "postinstall": "npm run tools:copy-vendors"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/seokju-na/geeks-diary.git"
    },
    "author": {
        "name": "Seokju Na",
        "email": "seokju.me@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/seokju-na/geeks-diary/issues"
    },
    "homepage": "https://github.com/seokju-na/geeks-diary#readme",
    "devDependencies": {
        "@angular/animations": "^5.2.10",
        "@angular/cdk": "^5.2.0",
        "@angular/cli": "~1.7.1",
        "@angular/common": "^5.2.10",
        "@angular/compiler": "^5.2.10",
        "@angular/compiler-cli": "^5.2.10",
        "@angular/core": "^5.2.10",
        "@angular/forms": "^5.2.10",
        "@angular/platform-browser": "^5.2.10",
        "@angular/platform-browser-dynamic": "^5.2.10",
        "@ngrx/effects": "^5.2.0",
        "@ngrx/store": "^5.2.0",
        "@ngrx/store-devtools": "^5.2.0",
        "@types/codemirror": "^0.0.55",
        "@types/fs-extra": "^5.0.2",
        "@types/highlight.js": "^9.12.3",
        "@types/jasmine": "^2.8.6",
        "@types/node": "^9.6.0",
        "@types/tinycolor2": "^1.4.0",
        "@types/uuid": "^3.4.3",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "3.x.x",
        "codelyzer": "^4.2.1",
        "codemirror": "^5.36.0",
        "copy-webpack-plugin": "~4.4.1",
        "core-js": "^2.5.3",
        "css-loader": "^0.28.11",
        "devicon": "^2.0.0",
        "devicon-2.2": "^2.2.0",
        "electron": "^2.0.1",
        "electron-devtools-installer": "^2.2.3",
        "exports-loader": "^0.7.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "fs-extra": "^5.0.0",
        "highlight.js": "^9.12.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.1.0",
        "jasmine-core": "2.x.x",
        "json-loader": "^0.5.7",
        "karma": "^2.0.0",
        "karma-electron": "^5.2.2",
        "karma-electron-launcher": "^0.2.0",
        "karma-jasmine": "^1.1.1",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^3.0.0",
        "less": "^3.0.1",
        "less-loader": "^4.1.0",
        "line-awesome": "icons8/line-awesome",
        "lodash.camelcase": "^4.3.0",
        "marked": "^0.4.0",
        "monaco-editor": "^0.11.1",
        "optimize-js-plugin": "^0.0.4",
        "postcss-loader": "^2.1.3",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.6.2",
        "rxjs": "^5.5.6",
        "source-map-loader": "^0.2.3",
        "tinycolor2": "^1.4.1",
        "tslint": "^5.9.1",
        "typeface-open-sans": "^0.0.54",
        "typescript": "~2.5.3",
        "uuid": "^3.2.1",
        "webpack": "^3.0.0",
        "webpack-merge": "^4.1.2",
        "zone.js": "^0.8.26"
    },
    "build": {
        "appId": "me.seokju.geeks-diary",
        "productName": "Geeks Diary",
        "directories": {
            "buildResources": "src/resources/",
            "output": "build/",
            "app": "dist/"
        },
        "asar": true
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 5"
    }
}
